            <Font size="18.0" />
         </font>
      </Label>
      <FontIcon fx:id="doneIcon" iconColor="WHITE" iconLiteral="fas-check-circle" iconSize="15" opacity="0.0" GridPane.rowIndex="2">
         <GridPane.margin>
            <Insets left="100.0" />
         </GridPane.margin>
      </FontIcon>
   </children>
   <opaqueInsets>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </opaqueInsets>
</GridPane>
