   <rowConstraints>
      <RowConstraints maxHeight="57.39999008178711" minHeight="0.0" prefHeight="0.0" valignment="CENTER" vgrow="SOMETIMES" />
      <RowConstraints maxHeight="146.8000061035156" minHeight="10.0" prefHeight="116.80001373291014" vgrow="SOMETIMES" />
      <RowConstraints maxHeight="119.2" minHeight="10.0" prefHeight="39.99998626708984" vgrow="SOMETIMES" />
      <RowConstraints maxHeight="65.79997558593752" minHeight="10.0" prefHeight="61.199987792968756" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <Button fx:id="doneButton" disable="true" mnemonicParsing="false" onMouseClicked="#OnAddNewGroupClicked" prefHeight="42.0" prefWidth="144.0" text="DONE" GridPane.rowIndex="3">
